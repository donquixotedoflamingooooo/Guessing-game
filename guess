import random

random_number = random.randint(1,100)
attempts = 3

for attempt in range(1, attempts+1):
    a=int(input('Enter your final guess: '))
    if a == random_number:
        print('You guessed the correct number')
        break
    elif a<random_number:
        print('The number you guessed is low')
    else:
        print('The number you guessed is high')
        


else:
    print(f'You ran out of your attempts. The correct number was {random_number}')
    
